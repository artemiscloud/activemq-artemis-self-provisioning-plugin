    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.7.3",
    "react-router-dom": "5.3.x",
    "react-router-dom-v5-compat": "6.22.3",
    "sort-package-json": "^2.10.0",
    "style-loader": "^3.3.1",
    "stylelint": "^15.3.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "^31.0.0",
    "swagger-routes-express": "^3.3.2",
    "ts-jest": "^29.2.1",
    "ts-loader": "^9.3.1",
    "ts-node": "10.9.2",
    "typescript": "^4.7.4",
    "webpack": "5.75.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "yaml": "^2.4.5"
  },
  "consolePlugin": {
    "name": "activemq-artemis-self-provisioning-plugin",
    "version": "0.1.0",
    "displayName": "Artemis Self Provisioning Plugin",
    "description": "Artemis Self Provisioning Plugin",
    "exposedModules": {
      "BrokersPage": "./brokers/view-brokers",
      "AddBrokerPage": "./brokers/add-broker",
      "UpdateBrokerPage": "./brokers/update-broker",
      "PodsListPage": "./brokers/broker-pods",
      "BrokerDetailsPage": "./brokers/broker-details",
      "AddressDetailsPage": "./addresses/address-details"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  },
  "readme": "README.md",
  "_id": "activemq-artemis-self-provisioning-plugin@0.1.0"
}
