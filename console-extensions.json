[
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": ["/k8s/ns/:ns/brokers", "/k8s/all-namespaces/brokers"],
      "component": { "$codeRef": "BrokersPage" }
    }
  },
  {
    "type": "console.page/route",
    "properties": {
      "exact": true,
      "path": ["/k8s/ns/:ns/brokers/:broker","k8s/all-namespaces/brokers/:broker"],
      "component": { "$codeRef": "ExampleTab" }
    }
  },
  {
    "type": "console.navigation/section",
    "properties": {
      "id": "activemq",
      "perspective": "admin",
      "name": "ActiveMQ Artemis",
      "dataAttributes": { "data-quickstart-id": "qs-nav-home" }
    }
  },
  {
    "type": "console.navigation/href",
    "properties": {
      "id": "brokers",
      "name": "ActiveMQ Artemis Self Provisioning",
      "href": "/k8s/all-namespaces/brokers",
      "perspective": "admin",
      "section": "activemq"
    }
  },
  {
    "type": "console.tab/horizontalNav",
    "properties": {
      "page": {
        "name": "ActiveMQ Artemis",
        "href": "artemis-tab"
      },
      "model": {
        "group": "core",
        "version": "v1",
        "kind": "Pod"
      },
      "component": { "$codeRef": "ExampleTab" }
    }
  }
]
